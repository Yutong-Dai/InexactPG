%% my example
p=20;
g=5;

v=randn(p,1);

lambda1=0.0;
lambda2=1.1;
Y=zeros(200,1);
maxIter=2000;
tol=1e-12;


%% ---- Example 1

G=[1:8, 6:12, 8:15, ]-1;
W=[ [1 20 1]', [21 40 1]', [41 60 1]', [61 80 1]', [81 100 1]',...
    [101 120 1]', [121 140 1]', [141 160 1]', [161 180 1]', [181 200 1]' ];
W(1:2,:)=W(1:2,:)-1;


Y=zeros(200,1);
tic;
[x,gap, infor]=overlapping(v,  p, g, lambda1, lambda2,...
    W, G, Y, maxIter, 2, tol);
toc;